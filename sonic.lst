00001482                            
00001482                            
00001482                            NemDec_ProcessCompressedData:
00001482 3E06                       		move.w	d6,d7
00001484 5147                       		subq.w	#8,d7	; get shift value
00001486 3205                       		move.w	d5,d1
00001488 EE69                       		lsr.w	d7,d1	; shift so that high bit of the code is in bit position 7
0000148A 0C01 00FC                  		cmpi.b	#%11111100,d1	; are the high 6 bits set?
0000148E 6400                       		bcc.s	NemPCD_InlineData	; if they are, it signifies inline data
00001490 0241 00FF                  		andi.w	#$FF,d1
00001494 D241                       		add.w	d1,d1
00001496 1031 1000                  		move.b	(a1,d1.w),d0	; get the length of the code in bits
0000149A 4880                       		ext.w	d0
0000149C 9C40                       		sub.w	d0,d6	; subtract from shift value so that the next code is read next time around
0000149E 0C46 0009                  		cmpi.w	#9,d6	; does a new byte need to be read?
000014A2 6400                       		bcc.s	loc_14B2	; if not, branch
000014A4 5046                       		addq.w	#8,d6
000014A6 E145                       		asl.w	#8,d5
000014A8 1A18                       		move.b	(a0)+,d5	; read next byte
000014AA                            
000014AA                            loc_14B2:
000014AA 1231 1001                  		move.b	1(a1,d1.w),d1
000014AE 3001                       		move.w	d1,d0
000014B0 0241 000F                  		andi.w	#$F,d1	; get palette index for pixel
000014B4 0240 00F0                  		andi.w	#$F0,d0
000014B8                            
000014B8                            NemPCD_ProcessCompressedData:
000014B8 E848                       		lsr.w	#4,d0	; get repeat count
000014BA                            
000014BA                            NemPCD_WritePixel:
