00001532                            
00001532                            NemBCT_Loop:
00001532 1018                       		move.b	(a0)+,d0	; read next byte
00001534 0C00 0080                  		cmpi.b	#$80,d0	; sign bit being set signifies a new palette index
00001538 64EE                       		bcc.s	NemBCT_ChkEnd	; a bmi could have been used instead of a compare and bcc
0000153A                            		
0000153A 1200                       		move.b	d0,d1
0000153C 0247 000F                  		andi.w	#$F,d7	; get palette index
00001540 0241 0070                  		andi.w	#$70,d1	; get repeat count for palette index
00001544 8E41                       		or.w	d1,d7	; combine the two
00001546 0240 000F                  		andi.w	#$F,d0	; get the length of the code in bits
