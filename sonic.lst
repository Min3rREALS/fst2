000014CE                            NemPCD_InlineData:
000014CE 5D46                       		subq.w	#6,d6	; 6 bits needed to signal inline data
000014D0 0C46 0009                  		cmpi.w	#9,d6
000014D4 6400                       		bcc.s	loc_14E4
000014D6 5046                       		addq.w	#8,d6
000014D8 E145                       		asl.w	#8,d5
000014DA 1A18                       		move.b	(a0)+,d5
000014DC                            
