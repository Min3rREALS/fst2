000014DE 3205                       		move.w	d5,d1
000014E0 EC69                       		lsr.w	d6,d1	; shift so that low bit of the code is in bit position 0
000014E2 3001                       		move.w	d1,d0
000014E4 0241 000F                  		andi.w	#$F,d1	; get palette index for pixel
000014E8 0240 0070                  		andi.w	#$70,d0	; high nybble is repeat count for pixel
000014EC 0C46 0009                  		cmpi.w	#9,d6
000014F0 64C6                       		bcc.s	NemPCD_ProcessCompressedData
000014F2 5046                       		addq.w	#8,d6
000014F4 E145                       		asl.w	#8,d5
000014F6 1A18                       		move.b	(a0)+,d5
000014F8 60BE                       		bra.s	NemPCD_ProcessCompressedData
