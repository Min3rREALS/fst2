00001554 6600                       		bne.s	NemBCT_ShortCode	; if not, a bit of extra processing is needed
00001556 1018                       		move.b	(a0)+,d0	; get code
00001558 D040                       		add.w	d0,d0	; each code gets a word-sized entry in the table
