00001562 E368                       		lsl.w	d1,d0	; get index into code table
00001564 D040                       		add.w	d0,d0	; shift so that high bit is in bit position 7
00001566 7A01                       		moveq	#1,d5
00001568 E36D                       		lsl.w	d1,d5
0000156A 5345                       		subq.w	#1,d5	; d5 = 2^d1 - 1
